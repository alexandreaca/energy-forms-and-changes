RectangularThermalMovableModelElement -> {Beaker, Block};
Beaker -> BeakerContainer;
Block -> {IronBlock, Brick};

subgraph "cluster_enums" {
  label="Enums:";
  EnergyType;
  EnergyContainerCategory;
}

TemperatureAndColor -> EnergyType [style="invis"];
}
